<Page
    x:Class="Dashboard.UI.Pages.PrintPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Dashboard.UI.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="PrintPage"
    d:DesignHeight="300"
    d:DesignWidth="300"
    Background="White"
    Loaded="Page_Loaded"
    mc:Ignorable="d">

    <Page.Resources>
        <FixedPage
            x:Key="fPage"
            x:Name="DESIGN_FixedPage"
            Width="357"
            Height="472"
            Background="Transparent">
            <Image
                x:Name="DESIGN_Image"
                Width="354"
                Height="472"
                Source="pack://application:,,,/Dashboard;component/Resources/Images/ReportDefault - NO.png" />
            <Canvas x:Name="DESIGN_Canvas" />
        </FixedPage>
    </Page.Resources>

    <DocumentViewer x:Name="DocViewer" ToolBarTray.IsLocked="True">
        <FixedDocument x:Name="Document" />
    </DocumentViewer>

</Page>
